{
  "name": "osp-cli",
  "version": "1.5.0",
  "description": "A CLI to simplify the process of creating common frontend projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/osmanthuspeace/osp-cli"
  },
  "type": "module",
  "scripts": {
    "test": "echo \"No tests yet\"",
    "prepare": "husky install",
    "commit": "git add . && pnpm cz && git push origin main"
  },
  "bin": {
    "osp": "./bin/osp-cli.js"
  },
  "keywords": [
    "cli",
    "react",
    "frontend",
    "project-setup",
    "template-generator"
  ],
  "author": "osmanthuspeace",
  "license": "MIT",
  "dependencies": {
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "minimist": "^1.2.8",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.0",
    "semantic-release": "^24.2.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
